datasource db {
  provider = "mongodb"
  url      = env("DB_URL")
}

generator client {
  provider = "prisma-client-js"
}

model ProcessedArticle {
  articleId   String?
  createdAt   DateTime  @default(now())
  id          String    @id @default(auto()) @map("_id") @db.ObjectId
  processedAt DateTime?
  updatedAt   DateTime  @updatedAt
}
